  "keywords": [
    "PhpPlaisio",
    "password"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
